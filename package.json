{
  "name": "zerberus",
  "version": "1.0.0",
  "description": "Software for a robot to chase racoons in your garden",
  "scripts": {
    "start": "concurrently -k 'npm:start-robot' 'npm:start-camera'",
    "start-robot": "sudo NODE_ENV=production npx ts-node robot | tee robot.log",
    "start-camera": "cd image_recognition && python3 use_classifier.py > ../camera.log",
    "simulator": "ts-node-dev --nolazy --watch --inspect -- robot",
    "lint": "eslint .",
    "test": "nyc --reporter=html --reporter=text-summary mocha",
    "build:types": "tsc -t es6 --moduleResolution node --outDir frontend types.ts && prettier -w ./frontend/types.js"
  },
  "keywords": [
    "rackoon",
    "self-driving car",
    "wild thumper",
    "robot"
  ],
  "engines": {
    "node": ">= 18"
  },
  "repository": {
    "url": "https://github.com/jschirrmacher/zerberus.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "7.6.0",
    "express": "4.18.2",
    "i2c-bus": "5.2.2",
    "pigpio": "3.3.1",
    "readline": "1.3.0",
    "socket.io": "4.5.4",
    "socket.io-client": "4.5.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/express": "4.17.16",
    "@types/i2c-bus": "5.1.0",
    "@types/mocha": "10.0.1",
    "@types/sinon": "10.0.13",
    "@types/socket.io": "3.0.1",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect": "^29.2.2",
    "jsdom": "^20.0.0",
    "mocha": "10.2.0",
    "node-fetch": "3.3.0",
    "nyc": "15.1.0",
    "pre-commit": "1.2.2",
    "prettier": "2.8.3",
    "sinon": "14.0.2",
    "ts-node-dev": "2.0.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
